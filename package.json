{
  "name": "magento-graphql",
  "private": true,
  "version": "0.0.1",
  "description": "Official Magento 2 GraphQL Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && node scripts/protogen.js && gql-gen && tsc",
    "start": "node dist/index.js",
    "watch": "npm run build && echo -e '\\033c' && node scripts/dev.js",
    "clean": "rm -rf dist generated"
